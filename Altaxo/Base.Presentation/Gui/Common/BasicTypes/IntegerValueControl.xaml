<UserControl x:Class="Altaxo.Gui.Common.BasicTypes.IntegerValueControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:agc="clr-namespace:Altaxo.Gui.Common"
             mc:Ignorable="d" 
             >
    <agc:BigIntegerUpDown Value="{Binding Value, Mode=TwoWay}" Minimum="{Binding Minimum}" Maximum="{Binding Maximum}" MinimumReplacementText="{Binding MinimumReplacementText}" MaximumReplacementText="{Binding MaximumReplacementText}" IsGotoMinimumAndMaximumVisible="{Binding IsGotoMinimumAndMaximumVisible}" />
</UserControl>
