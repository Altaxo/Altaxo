<UserControl x:Class="Altaxo.Gui.Graph.Scales.Rescaling.AngularScaleRescaleConditionsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Altaxo.Gui.Graph.Scales.Rescaling;assembly=AltaxoBase"
             d:DataContext="{d:DesignInstance Type=local:AngularRescaleConditionsController}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ComboBox ItemsSource="{Binding Origin.Items}" SelectedItem="{Binding Origin.SelectedItem, Mode=TwoWay}"/>
    </Grid>
</UserControl>
