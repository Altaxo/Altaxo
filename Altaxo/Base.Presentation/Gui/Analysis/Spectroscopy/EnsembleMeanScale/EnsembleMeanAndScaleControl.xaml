<UserControl x:Class="Altaxo.Gui.Analysis.Spectroscopy.EnsembleMeanScale.EnsembleMeanAndScaleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Altaxo.Gui.Analysis.Spectroscopy.EnsembleMeanScale;assembly=AltaxoBase"
             d:DataContext="{d:DesignInstance Type=local:EnsembleMeanAndScaleController}"
             mc:Ignorable="d">
    <Grid>
        <CheckBox Content="Ensemble scaling" IsChecked="{Binding EnsembleScale, Mode=TwoWay}"/>
    </Grid>
</UserControl>
