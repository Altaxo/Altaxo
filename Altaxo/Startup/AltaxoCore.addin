					description="${res:XML.MainMenu.FileMenu.PageSetup.Description}" class="Altaxo.Graph.Commands.PageSetup" />
				<MenuItem id="PrintableSizeSetup" label="${res:XML.MainMenu.FileMenu.PrintableSizeSetup}" icon="Icons.16x16.NewDocumentIcon"
