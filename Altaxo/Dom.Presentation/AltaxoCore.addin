<AddIn name        = "Altaxo"
			 author      = "Dirk Lellinger"
			 copyright   = "(C) Dr. Dirk Lellinger"
			 url         = "http://altaxo.sourceforge.net"
			 description = "Altaxo module"
			 addInManagerHidden = "true">

    <Manifest>
        <Identity name = "Altaxo" version = "1.0.0.0" />
    </Manifest>

    <!--
  <StringResources file="..\data\resources\AltaxoString.resources" />
  <BitmapResources file="..\data\resources\AltaxoBitmap.resources" />
  -->

    <Runtime>
        <Import assembly=":WorkbenchCore">
            <Doozer name="OptionPanel" class="Altaxo.Gui.Workbench.OptionPanelDoozer" />
        </Import>

        <Import assembly=":WorkbenchBase">
            <ConditionEvaluator name="ActiveContentExtension" class="Altaxo.Gui.Workbench.ActiveContentExtensionConditionEvaluator" />
            <ConditionEvaluator name="ActiveViewContentUntitled" class="Altaxo.Gui.Workbench.ActiveViewContentUntitledConditionEvaluator" />
            <ConditionEvaluator name="ActiveWindowState" class="Altaxo.Gui.Workbench.ActiveWindowStateConditionEvaluator" />
            <ConditionEvaluator name="SolutionOpen" class="Altaxo.Gui.Workbench.SolutionOpenConditionEvaluator" />
            <ConditionEvaluator name="OpenWindowState" class="Altaxo.Gui.Workbench.OpenWindowStateConditionEvaluator" />
            <ConditionEvaluator name="WindowActive" class="Altaxo.Gui.Workbench.WindowActiveConditionEvaluator" />
            <ConditionEvaluator name="WindowOpen" class="Altaxo.Gui.Workbench.WindowOpenConditionEvaluator" />
            <ConditionEvaluator name="ProjectActive" class="Altaxo.Gui.Workbench.ProjectActiveConditionEvaluator" />
            <ConditionEvaluator name="CanNavigateBack" class="Altaxo.Gui.Workbench.CanNavigateBackConditionEvaluator" />
            <ConditionEvaluator name="CanNavigateForward" class="Altaxo.Gui.Workbench.CanNavigateForwardConditionEvaluator" />
            <ConditionEvaluator name="CompareProjectProperty" class="Altaxo.Gui.Workbench.CompareProjectPropertyConditionEvaluator" />

            <Doozer name="DialogPanel" class="Altaxo.Gui.Workbench.DialogPanelDoozer" />
            <Doozer name="DisplayBinding" class="Altaxo.Gui.Workbench.DisplayBindingDoozer" />
            <Doozer name="Pad" class="Altaxo.Gui.Workbench.PadDoozer" />
            <Doozer name="LanguageBinding" class="Altaxo.Gui.Workbench.LanguageBindingDoozer" />
            <Doozer name="Parser" class="Altaxo.Gui.Workbench.ParserDoozer" />
            <Doozer name="ProjectContentRegistry" class="Altaxo.Gui.Workbench.ProjectContentRegistryDoozer" />
        </Import>

        <Import assembly=":Workbench">
        </Import>

        <Import assembly=":AltaxoBase" />
        <Import assembly=":AltaxoBase.Presentation" />
        <Import assembly=":AltaxoDom" />
        <Import assembly=":AltaxoMarkdownEditing" />
        <Import assembly=":AltaxoDom.Presentation">
            <ConditionEvaluator name="SelectedDataColumns" class="Altaxo.Worksheet.Commands.SelectedDataConditionEvaluator" />
            <ConditionEvaluator name="SelectedPropertyColumns" class="Altaxo.Worksheet.Commands.SelectedPropertyConditionEvaluator" />
            <ConditionEvaluator name="ContainsTableDataSource" class="Altaxo.Worksheet.Commands.TableDataSourcePresentConditionEvaluator" />
            <ConditionEvaluator name="ContainsPLSModelData" class="Altaxo.Worksheet.Commands.PLSModelConditionEvaluator" />
            <ConditionEvaluator name="Clipboard" class="Altaxo.Main.Commands.ClipboardContentConditionEvaluator" />
            <ConditionEvaluator name="IsInEmbeddedObjectMode" class="Altaxo.Main.Commands.EmbeddedObjectModeEvaluator" />
            <ConditionEvaluator name="ProjbrowserListItemSelection" class="Altaxo.Gui.Pads.ProjectBrowser.ListItemSelectionEvaluator" />
            <Doozer name="Texture" class="Altaxo.Main.TextureDoozer" />
            <Doozer name="NamedColor" class="Altaxo.Main.NamedColorDoozer" />
            <Doozer name="ColorSet" class="Altaxo.Main.ColorSetDoozer" />
            <Doozer name="GraphExportBinding" class="Altaxo.Main.GraphExportBindingDoozer" />
        </Import>
    </Runtime>

    <!-- Altaxo stuff -->

    <Path name="/Altaxo/Services">
        <Service id="Altaxo.Main.IProjectService"
                             class="Altaxo.Main.ProjectService" />
        <Service id="Altaxo.Gui.WpfFontManager"
                             interface1="Altaxo.Graph.Gdi.GdiFontManager"
                             class="Altaxo.Gui.WpfFontManager" />
        <Service id="Altaxo.Main.Services.IRecentOpen"
                             class="Altaxo.Main.Services.RecentOpen" />
        <Service id="Altaxo.Main.Services.IShutdownService"
                             class="Altaxo.Gui.Workbench.ShutdownService" />
        <Service id="Altaxo.Main.Services.IInstanceStorageService"
                             class="Altaxo.Main.Services.InstanceStorageService" />
        <Service id="Altaxo.Main.Services.IAutoUpdateInstallationService"
                             class="Altaxo.Serialization.AutoUpdates.UpdateInstallerStarter" />
        <Service id="Altaxo.Main.Services.IGuiTimerService"
                             class="Altaxo.Main.Services.DispatcherTimerService" />
        <Service id="Altaxo.Main.Services.IStatusBarService"
                             class="Altaxo.Main.Services.StatusBarService" />
        <Service id="Altaxo.Gui.Units.UserDefinedUnitEnvironmentsService"
                             class="Altaxo.Gui.Units.UserDefinedUnitEnvironmentsService" />
        <Service id="Altaxo.Main.Services.IHighResolutionClock"
                             class="Altaxo.Main.Services.HighResolutionClock" />
        <Service id="Altaxo.Main.Services.ITimerQueue"
                             class="Altaxo.Main.Services.TimerQueue" />
        <Service id="Altaxo.Main.IComManager"
                             class="Altaxo.Com.AltaxoComManager" />
        <Service id="Altaxo.Main.IPrintingService"
                             class="Altaxo.Main.PrintingService" />
        <Service id="Altaxo.Main.Services.IFitFunctionService"
                             class="Altaxo.Main.Services.FitFunctionService" />
        <Service id="Altaxo.Main.Services.ScriptCompilation.IScriptCompilerService"
                         class="Altaxo.Main.Services.ScriptCompilation.RoslynScriptCompilerService" />
        <Service id="Altaxo.Main.Services.IGuiFactoryService"
                             interface1="Altaxo.Gui.GuiFactoryServiceWpfWin"
                             class="Altaxo.Gui.GuiFactoryServiceWpfWin" />
        <Service id="Altaxo.Main.Services.ILaTeXFormulaImageStreamProvider"
                             class="Altaxo.Gui.Markdown.LaTeXFormulaImageStreamProvider" />
        <Service id="Altaxo.Serialization.AutoUpdates.INetFrameworkVersionDetermination"
                             class="Altaxo.Main.Services.NetFrameworkVersionDetermination" />
    </Path>

    <Path name="/Altaxo/Autostart">
        <Class id="StartColorSetManager"
                             class="Altaxo.Gui.Autostart.StartColorSetManager" />
        <Class id="StartTextureManager"
                             class="Altaxo.Gui.Autostart.StartTextureManager" />
        <Class id="StartUpdateDownloader"
                             class="Altaxo.Gui.Autostart.StartUpdateDownloader" />
    </Path>

    <Path name = "/Altaxo/Workbench/Pads">

        <Pad id       = "FileScout"
                     category = "Main"
                     title    = "${res:MainWindow.Windows.FileScoutLabel}"
                     icon     = "PadIcons.FileBrowser"
                     shortcut = "Control|Alt|F"
                     defaultPosition = "Right"
                 class    = "Altaxo.Gui.Pads.FileBrowser.FileBrowserController" />

        <Pad id       = "AltaxoProjectScout"
                    category = "Main"
                    title    = "${res:MainWindow.Windows.ProjectScoutLabel}"
                    icon     = "PadIcons.ProjectBrowser"
                    shortcut = "Control|Alt|P"
                    precreated = "true"
                    defaultPosition = "Right"
                    class    = "Altaxo.Gui.Pads.ProjectBrowser.ProjectBrowseController" />

        <Pad id       = "AltaxoOutputWindow"
                    category = "Main"
                    title    = "${res:MainWindow.Windows.OutputWindow}"
                    icon     = "PadIcons.Output"
                    shortcut = "Control|Alt|O"
                    precreated ="true"
                    defaultPosition = "Bottom"
                    class    = "Altaxo.Gui.Pads.Output.TextOutputWindowController" />

        <Pad id       = "AltaxoDataDisplayWindow"
                    category = "Main"
                    title    = "${res:MainWindow.Windows.DataDisplayWindow}"
                    icon     = "Icons.16x16.ReadData"
                    shortcut = "Control|Alt|D"
                    precreated ="true"
                    defaultPosition = "Bottom"
                    class    = "Altaxo.Gui.Pads.DataDisplay.DataDisplayController" />

        <Pad id       = "AltaxoNotesWindow"
                 category = "Main"
                 title    = "${res:MainWindow.Windows.AltaxoNotesWindowLabel}"
                 icon     = "Icons.16x16.Notepad"
                 shortcut = "Control|Alt|N"
                 precreated ="true"
                 defaultPosition = "Bottom"
                 class    = "Altaxo.Gui.Pads.Notes.NotesController" />

        <Pad id       = "AltaxoLightingWindow"
             category = "Main"
             title    = "${res:MainWindow.Windows.AltaxoLightingWindowLabel}"
             icon     = "Icons.16x16.Lighting"
             shortcut = "Control|Alt|L"
             precreated ="true"
             defaultPosition = "Right"
             class    = "Altaxo.Gui.Pads.LightingPad.LightingController" />
    </Path>

    <Path name = "/Altaxo/Workbench/MainMenu">
        <!-- File menu -->
        <MenuItem id="File" label="${res:XML.MainMenu.FileMenu}" type="Menu">
            <MenuItem id="New" label="${res:XML.MainMenu.FileMenu.New}" type="Menu">
                <MenuItem id="StandardWorksheet" label="${res:XML.MainMenu.FileMenu.New.StandardWorksheet}"
                            icon="Icons.16x16.StandardWorksheet" description="${res:XML.MainMenu.FileMenu.New.StandardWorksheet.Description}"
                            shortcut="Control|W" class="Altaxo.Main.Commands.CreateNewStandardWorksheet" />
                <MenuItem id="EmptyWorksheet" label="${res:XML.MainMenu.FileMenu.New.Worksheet}" icon="Icons.16x16.EmptyWorksheet"
                            description="${res:XML.MainMenu.FileMenu.New.Worksheet.Description}" shortcut="Control|E"
                            class="Altaxo.Main.Commands.CreateNewWorksheet" />
                <MenuItem id="Graph" label="${res:XML.MainMenu.FileMenu.New.Graph}" icon="Icons.16x16.PlotLineScatter"
                            description="${res:XML.MainMenu.FileMenu.New.Graph.Description}" shortcut="Control|G"
                            class="Altaxo.Main.Commands.CreateNewGraph" />
                <MenuItem id="NewNote" label="${res:XML.MainMenu.FileMenu.New.TextDocument}" description="${res:XML.MainMenu.FileMenu.New.TextDocument.Description}"
                            icon="Icons.16x16.Editor" class="Altaxo.Main.Commands.CreateNewText" />
                <MenuItem id="WksGraphFromFile" label="${res:XML.MainMenu.FileMenu.New.WksGraphFromFile}"
                            icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.New.WksGraphFromFile.Description}"
                            shortcut="Control|N" class="Altaxo.Main.Commands.CreateNewWorksheetOrGraphFromFile" />
                <MenuItem id="InstanceScript" label="${res:XML.MainMenu.FileMenu.New.InstanceScript}"
                            icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.New.InstanceScript.Description}"
                            class="Altaxo.Main.Commands.NewInstanceScript" />
            </MenuItem>
            <Condition name="IsInEmbeddedObjectMode" value="false" action="Exclude">
                <MenuItem id="Open"
                                            label="${res:XML.MainMenu.FileMenu.Open}"
                                            description="${res:XML.MainMenu.FileMenu.Open.Description}"
                                            icon="Icons.16x16.OpenProjectIcon"
                                            shortcut="Control|O"
                                            class="Altaxo.Main.Commands.FileOpen" />
                <MenuItem id="Close" label="${res:XML.MainMenu.FileMenu.Close}" icon="Icons.16x16.CloseAllDocuments"
                            description="${res:XML.MainMenu.FileMenu.Close.Description}" class="Altaxo.Main.Commands.CloseProject" />
                <MenuItem id="CloseSeparator" type = "Separator" />
                <MenuItem id="SaveProject" label="${res:XML.MainMenu.FileMenu.SaveProject}" icon="Icons.16x16.SaveAllIcon"
                            description="${res:XML.MainMenu.FileMenu.SaveProject.Description}" shortcut="Control|S"
                            class="Altaxo.Main.Commands.FileSave" />
                <MenuItem id="SaveProjectAs" label="${res:XML.MainMenu.FileMenu.SaveProjectAs}" icon="Icons.16x16.SaveAllIcon"
                            description="${res:XML.MainMenu.FileMenu.SaveProjectAs.Description}" class="Altaxo.Main.Commands.FileSaveAs" />
                <MenuItem id="FileSpecial" label="${res:XML.MainMenu.FileMenuSpecial}" type="Menu">
                    <MenuItem id="OpenFromDir"
                              label="${res:XML.MainMenu.FileMenu.OpenFromDirectory}"
                              description="${res:XML.MainMenu.FileMenu.OpenFromDirectory.Description}"
                              icon="Icons.16x16.OpenProjectIcon"
                              class="Altaxo.Main.Commands.FileOpenFromDirectory" />
                    <MenuItem id="SaveProjectAsToDir"
                              label="${res:XML.MainMenu.FileMenu.SaveProjectAsToDirectory}"
                              icon="Icons.16x16.SaveAllIcon"
                      description="${res:XML.MainMenu.FileMenu.SaveProjectAsToDirectory.Description}" class="Altaxo.Main.Commands.FileSaveAsToDirectory" />
                </MenuItem>
            </Condition>
            <Condition name="IsInEmbeddedObjectMode" value="true" action="Exclude">
                <MenuItem id="SaveCopyOfProjectAs" label="${res:XML.MainMenu.FileMenu.SaveCopyOfProjectAs}" icon="Icons.16x16.SaveAllIcon"
                            description="${res:XML.MainMenu.FileMenu.SaveCopyOfProjectAs.Description}" class="Altaxo.Main.Commands.FileSaveCopyAs" />
            </Condition>
            <MenuItem id="NewDocId" label="${res:XML.MainMenu.FileMenu.NewDocumentIdentifier}"
                      description="${res:XML.MainMenu.FileMenu.NewDocumentIdentifier.Description}" class="Altaxo.Main.Commands.NewDocumentIdentifier" />
            <MenuItem id="SaveProjectSeparator" type = "Separator" />
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Worksheet.Viewing.WorksheetController" action="Exclude">
                <MenuItem id="SaveWorksheetAs" label="${res:XML.MainMenu.FileMenu.SaveWorksheetAs}" icon="Icons.16x16.SaveIcon"
                            description="${res:XML.MainMenu.FileMenu.SaveWorksheetAs.Description}" class="Altaxo.Worksheet.Commands.SaveAs" />
                <MenuItem id="SaveWorksheetAsTemplate" label="${res:XML.MainMenu.FileMenu.SaveWorksheetAsTemplate}"
                            icon="Icons.16x16.SaveIcon" description="${res:XML.MainMenu.FileMenu.SaveWorksheetAsTemplate.Description}"
                            class="Altaxo.Worksheet.Commands.SaveAsTemplate" />
            </Condition>
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Graph.Gdi.Viewing.GraphController" action="Exclude">
                <MenuItem id="SaveGraphAsTemplate" label="${res:XML.MainMenu.FileMenu.SaveGraphAsTemplate}"
                            icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.SaveGraphAsTemplate.Description}"
                            class="Altaxo.Graph.Commands.SaveGraphAsTemplate" />
                <MenuItem id="SaveGraphMiniProject" label="${res:XML.MainMenu.FileMenu.SaveGraphAsMiniProject}"
                            icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.SaveGraphAsMiniProject.Description}"
                            class="Altaxo.Graph.Commands.SaveAsMiniProject" />
            </Condition>
            <MenuItem id="SaveWindowSeparator"  type = "Separator" />
            <MenuItem id="PrintSeparator" type = "Separator" />
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Graph.Gdi.Viewing.GraphController" action="Exclude">
                <MenuItem id="PrintOptionsSetup" label="${res:XML.MainMenu.FileMenu.PrintOptionsSetup}" icon="Icons.16x16.NewDocumentIcon"
                            description="${res:XML.MainMenu.FileMenu.PrintOptionsSetup.Description}" class="Altaxo.Graph.Commands.PrintOptionsSetup" />
                <MenuItem id="GraphPrint" label="${res:XML.MainMenu.FileMenu.Print}" icon="Icons.16x16.Printer"
                            description="${res:XML.MainMenu.FileMenu.Print.Description}" class="Altaxo.Graph.Commands.Print" />
            </Condition>
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Text.Viewing.TextDocumentController" action="Exclude">
                <MenuItem id="TextPrint" label="${res:XML.MainMenu.FileMenu.Print}" icon="Icons.16x16.Printer"
                            description="${res:XML.MainMenu.FileMenu.Print.Description}" class="Altaxo.Main.MenuCommands.Text.Print" />
            </Condition>
            <MenuItem id="ImportExportSeparator" type = "Separator" />
            <MenuItem id="Import" label="${res:XML.MainMenu.FileMenu.Import}" type="Menu">
                <MenuItem id="FileImportAscii" label="${res:XML.MainMenu.FileMenu.Import.Ascii}" icon="Icons.16x16.ImportAscii"
                                 description="${res:XML.MainMenu.FileMenu.Import.Ascii.Description}" class="Altaxo.Main.Commands.FileImportAscii" />
                <MenuItem id="FileImportAsciiWithOptions" label="${res:XML.MainMenu.FileMenu.Import.AsciiWithOptions}" icon="Icons.16x16.ImportAscii"
                                 description="${res:XML.MainMenu.FileMenu.Import.AsciiWithOptions.Description}" class="Altaxo.Main.Commands.FileImportAsciiWithOptions" />
                <MenuItem id="TextImport" label="${res:XML.MainMenu.FileMenu.ImportMarkdown}"
                                            description="${res:XML.MainMenu.FileMenu.ImportMarkdown.Description}" class="Altaxo.Main.MenuCommands.Text.ImportMarkdown" />

                <Condition name = "WindowActive" activewindow="Altaxo.Gui.Worksheet.Viewing.WorksheetController" action="Exclude">
                    <MenuItem id="WorksheetImportAsciiSingleWksHorz" label="${res:XML.MainMenu.FileMenu.Import.AsciiSingleWorksheet}" icon="Icons.16x16.ImportAscii"
                              description="${res:XML.MainMenu.FileMenu.Import.AsciiSingleWorksheet.Description}" class="Altaxo.Worksheet.Commands.ImportAsciiInSingleWorksheetHorizontally" />
                    <MenuItem id="WorksheetImportAsciiSingleWksVert" label="${res:XML.MainMenu.FileMenu.Import.AsciiSingleWorksheetVert}" icon="Icons.16x16.ImportAscii"
                                  description="${res:XML.MainMenu.FileMenu.Import.AsciiSingleWorksheetVert.Description}" class="Altaxo.Worksheet.Commands.ImportAsciiInSingleWorksheetVertically" />
                    <MenuItem id="WorksheetImportGalacticSPC" label="${res:XML.MainMenu.FileMenu.Import.GalacticSPC}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.GalacticSPC.Description}"
                                  class="Altaxo.Worksheet.Commands.ImportGalacticSPC" />
                    <MenuItem id="WorksheetImportJcamp" label="${res:XML.MainMenu.FileMenu.Import.Jcamp}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.Jcamp.Description}"
                                  class="Altaxo.Worksheet.Commands.ImportJcamp" />
                    <MenuItem id="WorksheetImportRenishawWdf" label="${res:XML.MainMenu.FileMenu.Import.RenishawWdf}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.RenishawWdf.Description}"
                                  class="Altaxo.Worksheet.Commands.ImportRenishawWdf" />
                    <MenuItem id="WorksheetImportOriginOpj" label="${res:XML.MainMenu.FileMenu.Import.OriginOpj}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.OriginOpj.Description}"
                                  class="Altaxo.Main.Commands.FileImportOriginOpj" />
                    <MenuItem id="WorksheetImportImage" label="${res:XML.MainMenu.FileMenu.Import.ImageData}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.ImageData.Description}"
                                  class="Altaxo.Worksheet.Commands.ImportImage" />
                    <MenuItem id="WorksheetFileImportScript" label="${res:XML.MainMenu.FileMenu.Import.UsingFileImportScript}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Import.UsingFileImportScript.Description}"
                                  class="Altaxo.Worksheet.Commands.OpenFileImportScriptDialog" />
                </Condition>
            </MenuItem>
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Worksheet.Viewing.WorksheetController" action="Exclude">
                <MenuItem id="Export" label="${res:XML.MainMenu.FileMenu.Export}" type="Menu">
                    <MenuItem id="WorksheetExportAscii" label="${res:XML.MainMenu.FileMenu.Export.Ascii}" icon="Icons.16x16.NewDocumentIcon"
                                  description="${res:XML.MainMenu.FileMenu.Export.Ascii.Description}" class="Altaxo.Worksheet.Commands.ExportAscii" />
                    <MenuItem id="WorksheetExportGalacticSPC" label="${res:XML.MainMenu.FileMenu.Export.GalacticSPC}"
                                  icon="Icons.16x16.NewDocumentIcon" description="${res:XML.MainMenu.FileMenu.Export.GalacticSPC.Description}"
                                  class="Altaxo.Worksheet.Commands.ExportGalacticSPC" />
                </MenuItem>
            </Condition>
            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Graph.Gdi.Viewing.GraphController" action="Exclude">
                <MenuItem id="GraphImportExportSeparator"  type = "Separator" />
                <MenuItem id="GraphExport" label="${res:XML.MainMenu.FileMenu.Export}" type="Menu">
                    <MenuItem id="GraphExportMetafile" label="${res:XML.MainMenu.FileMenu.Export.WindowsMetaFile}" icon="Icons.16x16.NewDocumentIcon"
                                  description="${res:XML.MainMenu.FileMenu.Export.WindowsMetaFile.Description}" class="Altaxo.Graph.Commands.FileExportMetafile" />
                    <MenuItem id="GraphExportTiff" label="${res:XML.MainMenu.FileMenu.Export.BitmapTiffFile}" icon="Icons.16x16.NewDocumentIcon"
                                  description="${res:XML.MainMenu.FileMenu.Export.BitmapTiffFile.Description}" class="Altaxo.Graph.Commands.FileExportTiff" />
                    <MenuItem id="GraphExportSpecific" label="${res:XML.MainMenu.FileMenu.Export.SpecificFile}" icon="Icons.16x16.NewDocumentIcon"
                               description="${res:XML.MainMenu.FileMenu.Export.SpecificFile.Description}" class="Altaxo.Graph.Commands.FileExportSpecific" />
                </MenuItem>
            </Condition>

            <Condition name = "WindowActive" activewindow="Altaxo.Gui.Text.Viewing.TextDocumentController" action="Exclude">
                <MenuItem id="TextImportExportSeparator"  type = "Separator" />
                <MenuItem id="TextExport" label="${res:XML.MainMenu.FileMenu.ExportMarkdown}"
                            description="${res:XML.MainMenu.FileMenu.ExportMarkdown.Description}" class="Altaxo.Main.MenuCommands.Text.ExportMarkdown" />
                <MenuItem id="ExportTextAsMaml"
                                                label="${res:XML.MainMenu.TextMenu.ExportMaml}"
                                                description="${res:XML.MainMenu.TextMenu.ExportMaml.Description}"
                                                class="Altaxo.Main.MenuCommands.Text.ExportMaml" />
                <MenuItem id="ExportTextAsHtml"
                                                label="${res:XML.MainMenu.TextMenu.ExportHtml}"
                                                description="${res:XML.MainMenu.TextMenu.ExportHtml.Description}"
                                                class="Altaxo.Main.MenuCommands.Text.ExportHtml" />
            </Condition>

            <MenuItem id="AfterExportSeparator" type = "Separator" />

            <Condition name="IsInEmbeddedObjectMode" value="false" action="Exclude">
                <MenuItem id = "RecentProjects" attribute = "RecentProjects" label = "${res:XML.MainMenu.FileMenu.RecentProjects}" type="Menu">
                    <MenuItem id = "RecentProjectsBuilder" type = "Builder" class = "Altaxo.Main.Commands.RecentProjectsMenuBuilder" />
                    <MenuItem id = "RecentProjectsSeparator" type = "Separator" />
                    <MenuItem id = "ClearRecentFiles"
                                                  label = "${res:XML.MainMenu.FileMenu.ClearRecentProjects}"
                                                  class = "Altaxo.Gui.Workbench.Commands.ClearRecentProjects" />
                </MenuItem>
            </Condition>
            <MenuItem id="ExitSeparator" type = "Separator" />
            <Condition name="IsInEmbeddedObjectMode" value="false" action="Exclude">
                <MenuItem id="Exit" label="${res:XML.MainMenu.FileMenu.Exit}" description="${res:XML.MainMenu.FileMenu.Exit.Description}"
                        class="Altaxo.Main.Commands.FileExit" />
            </Condition>
            <Condition name="IsInEmbeddedObjectMode" value="true" action="Exclude">
                <MenuItem id="Exit" label="${res:XML.MainMenu.FileMenu.ExitAndReturn}" description="${res:XML.MainMenu.FileMenu.ExitAndReturn.Description}"
                    class="Altaxo.Main.Commands.FileExit" />
            </Condition>
        </MenuItem>
        <!-- Edit menu -->
        <MenuItem id="Edit" label="${res:XML.MainMenu.EditMenu}"  type="Menu">
            <MenuItem id="Cut" label="${res:XML.MainMenu.EditMenu.Cut}" icon="Icons.16x16.CutIcon" description="${res:XML.MainMenu.EditMenu.Cut.Description}"
                      shortcut="Control|X" commands="Cut" />
            <MenuItem id="Copy" label="${res:XML.MainMenu.EditMenu.Copy}" icon="Icons.16x16.CopyIcon"
                      description="${res:XML.MainMenu.EditMenu.Copy.Description}" shortcut="Control|C" command="Copy" />
            <MenuItem id="Paste" label="${res:XML.MainMenu.EditMenu.Paste}" icon="Icons.16x16.PasteIcon"
                      description="${res:XML.MainMenu.EditMenu.Paste.Description}" shortcut="Control|V" command="Paste" />
            <MenuItem id="Delete" label="${res:XML.MainMenu.EditMenu.Delete}" icon="Icons.16x16.DeleteIcon"
                      description="${res:XML.MainMenu.EditMenu.Delete.Description}" shortcut="Delete" command="Delete" />
            <MenuItem id="SelectAll" label="${res:XML.MainMenu.EditMenu.SelectAll}" description="${res:XML.MainMenu.EditMenu.SelectAll.Description}"
                      shortcut="Control|A" command="SelectAll" />
            <MenuItem id="CopySpec" label="${res:XML.MainMenu.CopySpecificMenu}"  type="Menu">
                <Condition name = "WindowActive" activewindow="Altaxo.Gui.Graph.Gdi.Viewing.GraphController" action="Exclude">
                    <MenuItem id="CopyGraphActiveLayer" label="${res:XML.MainMenu.EditMenu.CopyGraphActiveLayer}" description="${res:XML.MainMenu.EditMenu.CopyGraphActiveLayer.Description}"
                                  class="Altaxo.Graph.Commands.CopyActiveLayer" />
                    <MenuItem id="CopyGraphAsMiniProject" label="${res:XML.MainMenu.EditMenu.CopyGraphAsMiniProject}" description="${res:XML.MainMenu.EditMenu.CopyGraphAsMiniProject.Description}"
                                  class="Altaxo.Graph.Commands.CopyAsMiniProjectToClipboard" />
                </Condition>
                <Condition name = "WindowActive" activewindow="Altaxo.Gui.Text.Viewing.TextDocumentController" action="Exclude">
                    <MenuItem id="CopyTextWithImages" label="${res:XML.MainMenu.EditMenu.CopyTextWithImages}" description="${res:XML.MainMenu.EditMenu.CopyTextWithImages.Description}"
                                  class="Altaxo.Main.MenuCommands.Text.CopyTextWithImages" />
                </Condition>
            </MenuItem>
